
<nav class="navbar navbar-inverse">
  <%= link_to 'Edit profile', edit_user_registration_path , class: "navbar-text" %>
  <%= link_to 'Logout', destroy_user_session_path, method: :delete, class: "navbar-text" %>

</nav>
<h1>Stuffs</h1>

<table class="table table-striped table-inverse">
  <tr>
    <th scope="row">Name</th>
    <th scope="row">Price</th>
    <th scope="row">Description</th>
    <th scope="row">Duration</th>
  </tr>
  <% @services.each do |service| %>
      <tr colspan='4'> <h1><%= service.name.capitalize %> service providers </h1></tr>
      <% service.service_items.each do |service_item| %>
          <tr>
            <td><%= service_item.name %></td>
            <td><%= service_item.price %></td>
            <td><%= service_item.description %></td>
            <td><%= service_item.duration %></td>
          </tr>
      <% end %>
  <% end %>
</table>